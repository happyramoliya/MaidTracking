<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shreeji.maidtracking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Maid Tracking"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity_Files.Login_activity" />
        <activity android:name=".Activity_Files.RegisterActivity" />
        <activity android:name=".Activity_Files.BookingActivity">
        </activity>
        <activity
            android:name=".Navigation.NavigationActivity"
            android:label="@string/title_activity_navigation"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".Activity_Files.ContactActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>